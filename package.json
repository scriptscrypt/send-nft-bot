{
  "name": "send-nft-bot",
  "version": "1.0.0",
  "description": "Telegram bot for generating images and minting them as NFTs on Solana",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "dev": "NODE_OPTIONS='--loader ts-node/esm' ts-node-dev --respawn src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js"
  },
  "keywords": [
    "telegram",
    "bot",
    "openai",
    "image-generation",
    "solana",
    "blockchain",
    "nft"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@langchain/core": "^0.3.26",
    "@langchain/langgraph": "^0.2.36",
    "@langchain/openai": "^0.3.16",
    "@privy-io/server-auth": "^1.0.0",
    "@solana-agent-kit/plugin-defi": "^2.0.2",
    "@solana-agent-kit/plugin-misc": "^2.0.2",
    "@solana-agent-kit/plugin-nft": "2.0.2",
    "@solana-agent-kit/plugin-token": "2.0.2",
    "@solana/web3.js": "^1.87.6",
    "@supabase/supabase-js": "^2.39.0",
    "dotenv": "^16.3.1",
    "node-fetch": "^2.7.0",
    "openai": "^4.20.1",
    "solana-agent-kit": "^2.0.2",
    "telegraf": "^4.15.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "overrides": {
    "node-fetch": "^2.7.0"
  },
  "resolutions": {
    "node-fetch": "^2.7.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.4",
    "@types/node-fetch": "^2.6.11",
    "nodemon": "^3.1.10",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.3"
  },
  "nodemonConfig": {
    "ignore": [
      "node_modules/*",
      "generated_images/*"
    ],
    "delay": "1000",
    "env": {
      "NODE_OPTIONS": "--experimental-json-modules --no-warnings"
    }
  }
}
