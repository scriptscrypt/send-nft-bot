  "version": "1.0.0",
  "description": "Telegram bot for generating images and minting them as NFTs on Solana",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "dev": "tsx src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js"
  },
  "keywords": [
    "telegram",
    "bot",
    "openai",
    "image-generation",
    "solana",
    "blockchain",
    "nft"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@ai-sdk/openai": "^1.3.21",
    "@privy-io/server-auth": "^1.0.0",
    "@solana-agent-kit/plugin-defi": "^2.0.2",
    "@solana-agent-kit/plugin-misc": "^2.0.2",
    "@solana-agent-kit/plugin-nft": "2.0.2",
    "@solana-agent-kit/plugin-token": "2.0.2",
    "@solana/web3.js": "^1.87.6",
    "@supabase/supabase-js": "^2.39.0",
    "ai": "^4.3.13",
    "dotenv": "^16.3.1",
    "node-fetch": "^2.7.0",
    "solana-agent-kit": "^2.0.2",
    "telegraf": "^4.15.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "overrides": {
    "node-fetch": "^2.7.0"
  },
  "resolutions": {
    "node-fetch": "^2.7.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.4",
    "@types/node-fetch": "^2.6.11",
    "nodemon": "^3.1.10",
    "tsx": "^4.19.4",
    "typescript": "^5.3.3"
  },
  "nodemonConfig": {
    "ignore": [
      "node_modules/*",
      "generated_images/*"
    ],
    "delay": "1000",
    "env": {
      "NODE_OPTIONS": "--experimental-json-modules --no-warnings"
    }
  },
  "packageManager": "pnpm@9.15.3+sha512.1f79bc245a66eb0b07c5d4d83131240774642caaa86ef7d0434ab47c0d16f66b04e21e0c086eb61e62c77efc4d7f7ec071afad3796af64892fae66509173893a"
